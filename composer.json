{
  "name": "smplygd/sg-replacement",
  "type": "typo3-cms-extension",
  "version": "1.0.0",
  "description": "Extension Replaces Placeholder with values",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Daniel Hoffmann",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4.0"
  },
  "extra": {
    "typo3/cms": {
      "sg-replacement": "sg_replacement"
    }
  },
  "replace":{
    "sg_replacement": "self.version",
    "typo3-ter/sg-replacement": "self.version"
  },
  "autoload":{
    "psr-4":{
      "SG\\SgReplacement\\": "Classes/"
    }
  }
}
